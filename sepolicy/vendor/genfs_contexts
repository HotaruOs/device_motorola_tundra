# Health
genfscon sysfs /devices/platform/soc/soc:qcom,pmic_glink/soc:qcom,pmic_glink:qcom,battery_charger/force_charging_disable    u:object_r:vendor_sysfs_battery_supply:s0

# LiveDisplay
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display-primary/dsi_display_dc              u:object_r:sysfs_livedisplay_tuneable:s0
